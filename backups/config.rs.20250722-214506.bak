use serde::{Deserialize, Serialize};
use std::path::PathBuf;

#[derive(Serialize, Deserialize, Debug, Default, Clone)]
pub struct Config {
    pub width: u32,
    pub height: u32,
    pub preset_path: PathBuf,
    pub texture_path: PathBuf,
    pub frame_rate: u32,
    pub preset_duration: f64,
    pub beat_sensitivity: f32,
}